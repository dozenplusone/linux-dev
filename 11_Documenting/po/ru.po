#: src/guesser.c:32
#, c-format
msgid ""
"Usage: %s [-h | --help | -r]\n"
"Guess a picked number.\n"
"\n"
"  -r                print numerals in Roman\n"
"  -h, --help        display this help and exit\n"
msgstr ""
"Использование: %s [-h | --help | -r]\n"
"Отгадает задуманное число.\n"
"\n"
"  -r                печатать числа римскими цифрами\n"
"  -h, --help        показать эту справку и выйти\n"

#: src/guesser.c:41
#, c-format
msgid ""
"%1$s: invalid option -- '%2$s'\n"
"Try '%1$s --help' for more information.\n"
msgstr ""
"%1$s: неверный ключ — «%2$s»\n"
"По команде «%1$s --help» можно получить дополнительную информацию.\n"

#: src/guesser.c:52
#, c-format
msgid "Pick a natural number between %s and %s and press Enter. "
msgstr "Загадайте натуральное число от %s до %s и нажмите Enter. "

#: src/guesser.c:57
#, c-format
msgid "Pick a natural number between %d and %d and press Enter. "
msgstr "Загадайте натуральное число от %d до %d и нажмите Enter. "

#: src/guesser.c:74
#, c-format
msgid "Is the number not greater than %s? [yes/no] "
msgstr "Число не превосходит %s? [да/нет] "

#: src/guesser.c:77
#, c-format
msgid "Is the number not greater than %d? [yes/no] "
msgstr "Число не превосходит %d? [да/нет] "

#: src/guesser.c:86
msgid "yes\n"
msgstr "да\n"

#: src/guesser.c:88
msgid "no\n"
msgstr "нет\n"

#: src/guesser.c:95
#, c-format
msgid "The answer is %s.\n"
msgstr "Ответ: %s.\n"

#: src/guesser.c:98
#, c-format
msgid "The answer is %d.\n"
msgstr "Ответ: %d.\n"
